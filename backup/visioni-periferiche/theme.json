{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "appearanceTools": true,
        "color": {
            "palette": [
                {
                    "slug": "black",
                    "color": "#000000",
                    "name": "Black"
                },
                {
                    "slug": "white",
                    "color": "#ffffff",
                    "name": "White"
                },
                {
                    "slug": "gold",
                    "color": "#ffd700",
                    "name": "Gold"
                },
                {
                    "slug": "dark-gray",
                    "color": "#1a1a1a",
                    "name": "Dark Gray"
                },
                {
                    "slug": "light-gray",
                    "color": "#f5f5f5",
                    "name": "Light Gray"
                }
            ],
            "gradients": [],
            "duotone": []
        },
        "typography": {
            "fontFamilies": [
                {
                    "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif",
                    "slug": "system-sans",
                    "name": "System Sans"
                }
            ],
            "fontSizes": [
                {
                    "slug": "small",
                    "size": "0.875rem",
                    "name": "Small"
                },
                {
                    "slug": "medium",
                    "size": "1rem",
                    "name": "Medium"
                },
                {
                    "slug": "large",
                    "size": "1.25rem",
                    "name": "Large"
                },
                {
                    "slug": "x-large",
                    "size": "1.5rem",
                    "name": "Extra Large"
                },
                {
                    "slug": "huge",
                    "size": "2.5rem",
                    "name": "Huge"
                },
                {
                    "slug": "gigantic",
                    "size": "3.5rem",
                    "name": "Gigantic"
                }
            ]
        },
        "spacing": {
            "units": ["px", "em", "rem", "%", "vh", "vw"],
            "spacingSizes": [
                {
                    "slug": "30",
                    "size": "0.5rem",
                    "name": "30"
                },
                {
                    "slug": "40",
                    "size": "1rem",
                    "name": "40"
                },
                {
                    "slug": "50",
                    "size": "1.5rem",
                    "name": "50"
                },
                {
                    "slug": "60",
                    "size": "2rem",
                    "name": "60"
                },
                {
                    "slug": "70",
                    "size": "3rem",
                    "name": "70"
                },
                {
                    "slug": "80",
                    "size": "5rem",
                    "name": "80"
                }
            ]
        },
        "layout": {
            "contentSize": "1200px",
            "wideSize": "1400px"
        },
        "custom": {
            "spacing": {
                "section": "clamp(3rem, 10vw, 6rem)"
            },
            "transitions": {
                "default": "all 0.3s ease"
            }
        }
    },
    "styles": {
        "color": {
            "background": "var(--wp--preset--color--black)",
            "text": "var(--wp--preset--color--white)"
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--system-sans)",
            "fontSize": "var(--wp--preset--font-size--medium)",
            "lineHeight": "1.6"
        },
        "spacing": {
            "blockGap": "var(--wp--preset--spacing--50)",
            "padding": {
                "top": "0",
                "right": "var(--wp--preset--spacing--40)",
                "bottom": "0",
                "left": "var(--wp--preset--spacing--40)"
            }
        },
        "elements": {
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--gold)"
                },
                ":hover": {
                    "color": {
                        "text": "var(--wp--preset--color--white)"
                    }
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--gigantic)",
                    "fontWeight": "700",
                    "lineHeight": "1.2"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--huge)",
                    "fontWeight": "700",
                    "lineHeight": "1.3"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--x-large)",
                    "fontWeight": "600",
                    "lineHeight": "1.4"
                }
            },
            "button": {
                "color": {
                    "background": "var(--wp--preset--color--gold)",
                    "text": "var(--wp--preset--color--black)"
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)",
                    "fontWeight": "600"
                },
                "spacing": {
                    "padding": {
                        "top": "0.75rem",
                        "right": "2rem",
                        "bottom": "0.75rem",
                        "left": "2rem"
                    }
                },
                "border": {
                    "radius": "0"
                }
            }
        },
        "blocks": {
            "core/navigation": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)"
                }
            }
        }
    },
    "templateParts": [
        {
            "name": "header",
            "title": "Header",
            "area": "header"
        },
        {
            "name": "footer",
            "title": "Footer",
            "area": "footer"
        }
    ],
    "customTemplates": [
        {
            "name": "landing",
            "title": "Landing Page",
            "postTypes": ["page"]
        }
    ]
}